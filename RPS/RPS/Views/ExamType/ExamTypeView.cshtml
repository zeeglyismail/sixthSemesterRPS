
@{
    ViewBag.Title = "ExamTypeView";
}


<input type="text" class="form-control" id="ExamTypeID" style="display:none">
<div align="center">

    <h2>Welcome to Exam Type List</h2>

    <form>

        <input type="text" class="form-control" id="DeptID" style="display:none">
        <div class="mb-3">
            <label class="form-label" style="font-size:larger">Exam Type Name</label>
            <input type="text" class="form-control" id="ExamTypeNameID" autocomplete="off">

            <label class="form-label" style="font-size:larger">Total Marks</label>
            <input type="text" class="form-control" id="TotalMarksID" autocomplete="off">

        </div>

        <div class="mb-3" style="margin-top:10px">
            <button type="button" class="btn btn-primary" onclick="Insert()">Submit</button>
            <button type="button" id="UpdateId" class="btn btn-primary" onclick="Update()">Update</button>
        </div>

        <div style="width: 800px;margin-top:10px">
            <table id="tblInfo" class="table table-bordered table-condensed table-hover table-striped">
                <thead>
                    <tr>
                        <th>Edit</th>
                        <th>Void</th>
                        <th>ExamType ID</th>
                        <th>ExamType Name</th>
                        <th>Total Marks</th>
                    </tr>
                </thead>
            </table>
        </div>

    </form>

</div>
<script src="~/Scripts/jquery-3.4.1.min.js"></script>
<script src="~/Scripts/ExamType/ExamType.js"></script>
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">