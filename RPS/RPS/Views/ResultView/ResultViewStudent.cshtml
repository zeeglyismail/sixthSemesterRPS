
@{
    ViewBag.Title = "ResultViewStudent";
}


<div align="center">
    <h2>Result View Here!!</h2>

    <label for="RegNo">RegNo:</label>
    <select name="RegNo" id="RegNo" onchange="StDataBind()">
        @*Js Theke Ashbe*@
    </select>

    <label for="StudentName">Student Name:</label>
    <input type="text" id="StudentName" readonly />

    <label for="DepartmentName">Department Name:</label>
    <input type="text" id="DepartmentName" readonly />
</div>

<div align="center">
    <div class="mb-3" style="margin-top:10px">
        <button type="button" class="btn btn-primary" onclick="Submit()">Submit</button>
    </div>
</div>

<div align="center">
    <div style="width: 1500px; margin-top: 100px;">
        <table id="tblStudent" class="table table-bordered table-condensed table-hover table-striped">
            <thead>
                <tr>
                    <th>ResultID</th>
                    <th>RegNo</th>
                    <th>Student Name</th>
                    <th>Department Name</th>
                    <th>Semester</th>
                    <th>Subject Name</th>
                    <th>Exam Name</th>
                    <th>Total Marks</th>
                    <th>Obtained Marks</th>
                </tr>
            </thead>
            <tbody>
            </tbody>
        </table>
    </div>
</div>


<script src="~/Scripts/jquery-3.4.1.min.js"></script>
<script src="~/Scripts/ResultView/ResultView.js"></script>
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">

